        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalProject"
        mc:Ignorable="d"
        Title="AddingPrefixSurfix" Height="450" Width="400">
    <Grid>
        <TextBlock x:Name="PrefixBlock" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Height="48" Width="152"/>
        <Button Content="Apply" HorizontalAlignment="Center" Margin="0,282,0,0" VerticalAlignment="Top" Height="41" Width="118" Click="Button_Click"/>

    </Grid>
</Window>
